doctype html
html
  head
    title Most Anticipated Shows
  body
    h1 Most Anticipated TV Shows
    table(border="1")
      thead
        tr
          th Show Title
          th Year
      tbody
        each show in shows
          tr
            td
              a(href=`https://imdb.com/title/${show.show.ids.imdb}`, target="_blank")= show.show.title
            td= show.show.year
